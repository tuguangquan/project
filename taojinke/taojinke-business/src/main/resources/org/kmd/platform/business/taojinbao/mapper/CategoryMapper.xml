<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.kmd.platform.business.taojinbao.mapper.CategoryMapper">
    <sql id="category_fields">
       id,uniacid,cid,bid,hid,hid2,hid3,tid,tid2,tid3,tid4,name,pic,displayorder,status,isindex,isgy,istopstatus,topurl,toppic,toptxt,toptags
    </sql>

    <resultMap id="CategoryResultMap" type="Category">
        <id property="id" column="id"/>
        <result property="uniacid" column="uniacid"/>
        <result property="cid" column="cid"/>
        <result property="bid" column="bid"/>
        <result property="hid" column="hid"/>
        <result property="hid2" column="hid2"/>
        <result property="hid3" column="hid3"/>
        <result property="tid" column="tid"/>
        <result property="tid2" column="tid2"/>
        <result property="tid3" column="tid3"/>
        <result property="tid4" column="tid4"/>
        <result property="name" column="name"/>
        <result property="pic" column="pic"/>
        <result property="displayorder" column="displayorder"/>
        <result property="status" column="status"/>
        <result property="isindex" column="isindex"/>
        <result property="isgy" column="isgy"/>
        <result property="istopstatus" column="istopstatus"/>
        <result property="topurl" column="topurl"/>
        <result property="toppic" column="toppic"/>
        <result property="toptxt" column="toptxt"/>
        <result property="toptags" column="toptags"/>
    </resultMap>


    <select id="get"   resultMap="CategoryResultMap">
        SELECT
        <include refid="category_fields"/>
        FROM ims_bsht_tbk_all_cate
    </select>


</mapper>